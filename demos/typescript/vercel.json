{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/oauth",
      "destination": "/api/oauth"
    },
    {
      "source": "/api/oauth.ts",
      "destination": "/api/oauth"
    },
    {
      "source": "/((?!api\/oauth).*)",
      "destination": "/index.html"
    }
  ]
}